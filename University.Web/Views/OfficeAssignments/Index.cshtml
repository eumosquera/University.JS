@{
    ViewBag.Title = "Index";
}
<h2>OfficeAssignments</h2>
<!-- Button trigger modal -->
<button id="create" type="button" class="btn btn-outline-primary">
    Add Office
</button>
<hr />
<div id="rowOfficeAssignments">
    <div id="divOfficeAssignments">
        <table id="tableOfficeAssignments" class="table table-bordered" style="width:100%"></table>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="modalOfficeAssignments" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">OfficeAssignments</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<script 
        src="~/Scripts/Views/OfficeAssignments/Index.js" type="text/javascript">   
</script>
