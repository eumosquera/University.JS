
@{
    ViewBag.Title = "Index";
}
<hr />
<!-- Button trigger modal -->
<button id="create" type="button" class="btn btn-success fas fa-user-plus">
    Add Instructor
</button>
<hr />

<div id="rowInstructors">
    <div id="divInstructors">
        <table id="tableInstructors" class="table table-bordered" style="width:100%"></table>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="modalInstructors" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Instructors</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<!-- Code Js-->
<script 
        src="~/Scripts/Views/Instructors/Index.js" type="text/javascript">
</script>