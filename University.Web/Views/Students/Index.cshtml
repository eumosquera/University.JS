
@{
    ViewBag.Title = "Index";
}
<h2>Students</h2>
<!-- Button trigger modal -->
<button id="create" type="button" class="btn btn-primary fas fa-user-graduate">
    Add
</button>
<hr />

<div id="rowStudents">
    <div id="divStudents">
        <table id="tableStudents" class="table table-bordered" style="width:100%"></table>
    </div>
</div>

<!-- Modal -->
<div class="modal fade" id="modalStudents" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Students</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body"></div>
            <div class="modal-footer">
                <button type="button" class="fas fa-times-circle" data-dismiss="modal"></button>
            </div>
        </div>
    </div>
</div>

<!--Java Script-->
<script 
        src="~/Scripts/Views/Students/Index.js" type="text/javascript">   
</script>

